{
    "$schema": "../gen/schemas/mobile-schema.json",
    "identifier": "mobile-capability",
    "windows": ["main"],
    "platforms": ["iOS", "android"],
    "permissions": [
        "path:default",
        "event:default",
        "window:default",
        "app:default",
        "resources:default",
        "menu:default",
        "tray:default",
        "window:allow-set-title",
        "dialog:allow-open",
        "dialog:allow-message",
        "menu:allow-get"
    ]
  }